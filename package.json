{
  "name": "host_bun_server",
  "module": "./src/index.ts",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "bun-types": "latest",
    "eslint": "^8.50.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "start": "bun run ./src/index.ts",
    "dev": "bun --hot ./src/index.ts",
    "builds": "bun build --minify --splitting --outdir=build ./src/**/* ./bin/**/* ./internal/**/*",
    "lint": "bun lintSrc && bun lintInternal",
    "lint-fix": "bun lintSrc --fix && bun lintInternal --fix",
    "lintSrc": "bun eslint --config ./bin/linter/.eslintrc.json ./src/**/*",
    "lintInternal": "bun eslint --config ./bin/linter/.eslintrc.json ./internal/**/*"
  },
  "type": "module",
  "dependencies": {
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  }
}
